{
    "require": {
        "lemon_framework/lemon": "dev-master",
        "cycle/orm": "^2.3",
        "cycle/annotated": "^3.2",
        "doctrine/annotations": "^1.14.3",
        "cycle/entity-behavior": "^1.1",
        "symfony/mailer": "^6.2",
        "mockery/mockery": "^1.5",
        "nesbot/carbon": "^2.68"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "phpstan/phpstan": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
